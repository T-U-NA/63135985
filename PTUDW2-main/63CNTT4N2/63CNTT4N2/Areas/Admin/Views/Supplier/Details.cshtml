@model MyClass.Model.Suppliers

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Details</h2>
<div class="wrapper">
    <!-- Main Sidebar Container -->
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Main content -->
        <section class="content">

            <!-- Default box -->
            <div class="card">
                <div class="card-header">
                    <div class="row">
                        <div class="col-md-6">
                            <strong class="text-danger">DANH MỤC NHÀ SẢN XUẤT</strong>
                        </div>
                        <div class="col-md-6 text-right">
                            <a href="@Url.Action("Edit", "Supplier", new { id = Model.Id })" class="btn-sm btn-success"><i class="fa fa-edit">Sửa</i></a>
                            <a href="@Url.Action("Index", "Supplier")" class="btn-sm btn-info"><i class="fa fa-long-arrow-alt-left">quay lại</i></a>

                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <table class="table table-bordered table-striped" id="myTable">
                        <thead>
                            <tr>
                                <th> @Html.DisplayNameFor(model => model.Id)</th>
                                <th> @Html.DisplayNameFor(model => model.Name)</th>
                                <th> @Html.DisplayNameFor(model => model.Image)</th>
                                <th> @Html.DisplayNameFor(model => model.Slug)</th>
                                <th> @Html.DisplayNameFor(model => model.Order)</th>
                                <th> @Html.DisplayNameFor(model => model.FullName)</th>
                                <th> @Html.DisplayNameFor(model => model.Phone)</th>
                                <th> @Html.DisplayNameFor(model => model.Email)</th>
                                <th> @Html.DisplayNameFor(model => model.UrlSite)</th>
                                <th> @Html.DisplayNameFor(model => model.MetaDesc)</th>
                                <th> @Html.DisplayNameFor(model => model.MetaKey)</th>
                                <th> @Html.DisplayNameFor(model => model.CreateAt)</th>
                                <th> @Html.DisplayNameFor(model => model.CreateBy)</th>
                                <th> @Html.DisplayNameFor(model => model.UpdateBy)</th>
                                <th> @Html.DisplayNameFor(model => model.UpdateAt)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td> @Html.DisplayFor(model => Model.Id)</td>
                                <td> @Html.DisplayFor(model => model.Name) </td>
                                <td> @Html.DisplayFor(model => model.Image) </td>
                                <td> @Html.DisplayFor(model => model.Slug)</td>
                                <td> @Html.DisplayFor(model => model.Order) </td>
                                <td> @Html.DisplayFor(model => model.FullName) </td>
                                <td> @Html.DisplayFor(model => model.Phone) </td>
                                <td> @Html.DisplayFor(model => model.Email) </td>
                                <td> @Html.DisplayFor(model => model.UrlSite) </td>
                                <td> @Html.DisplayFor(model => model.MetaDesc) </td>
                                <td> @Html.DisplayFor(model => model.MetaKey) </td>
                                <td> @Html.DisplayFor(model => model.CreateAt) </td>
                                <td> @Html.DisplayFor(model => model.CreateBy) </td>
                                <td> @Html.DisplayFor(model => model.UpdateAt) </td>
                                <td> @Html.DisplayFor(model => model.UpdateBy) </td>


                            </tr>

                        </tbody>

                    </table>
                </div> <!-- /.card-footer-->
            </div><!-- /.card -->
        </section><!-- /.content -->
    </div> <!-- /.content-wrapper -->
</div><!-- ./wrapper -->
